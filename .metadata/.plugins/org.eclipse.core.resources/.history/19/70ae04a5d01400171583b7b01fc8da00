package Fechas;
import java.util.Scanner;

public class Fechas {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner scn = new Scanner(System.in);
		FechaOperador fecha = new FechaOperador();
		
