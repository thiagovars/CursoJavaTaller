package EstructurasDeControl;

public class If {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int edad = 10;
		while (edad<13) {
			if (edad<10) {
				System.out.println("Eres menor de edad!");
			} else {
				System.out.println("Eres mayor de edad!");
			}
		}
	}

}
