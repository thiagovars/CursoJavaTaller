package EjercicioJAVA1;

public class Usuario {
	private String login;
	private String clave;
	
	public String getLogin() {
		return login;
	}
	public void setLogin(String login) {
		if () {
		  this.login = login;
		}
		
	}
	public String getClave() {
		return clave;
	}
	public void setClave(String clave) {
		this.clave = clave;
	}
	
	
}
