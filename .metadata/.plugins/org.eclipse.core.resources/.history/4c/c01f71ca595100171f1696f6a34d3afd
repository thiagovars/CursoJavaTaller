package CineJPanel;

public class user {

	public String name;
	private char[] passw;
	private ConexionDAO conn;
	
	public user(String nombre) {
		this.name = nombre;
	}
	
	public boolean validatePassw(char[] passw) {
		this.conn = new ConexionDAO();
		this.conn.validateUser(this.name, passw);
		return false;
	}
}
