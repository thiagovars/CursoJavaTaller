package CineJPanel;

public class user {

	public String name;
	private char[] passw;
	private ConexionDAO conn;
	
	public user(String nombre) {
		this.name = nombre;
	}
	
	public boolean validateLogin(String nombre, char[] passw) {
		this.conn = new ConexionDAO();
		System.out.println(this.conn.validateUser(nombre, passw));
		return false;
	}
}
